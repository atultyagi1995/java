abstract class Bike{
	Bike(){
		System.out.println("i love my bike");
	}
	int speedlimit(){
		return(110);
	}
	abstract int milage(); 
}
class Car extends Bike{
	int milage(){
	return(56);
	}

	public static void main(String[] args){
		Bike b=new Car();
	System.out.println("my bike milage is:"+b.milage()+ "\n speed limit is:"+b.speedlimit());
	}
}
